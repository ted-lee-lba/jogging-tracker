@using Microsoft.AspNet.Identity

<div ng-controller="NavBarController" ng-init="Init()">    
    <ul ng-show="IsLoggedIn" class="nav navbar-nav navbar-right">
        <li>
            <a href="#" ng-click="$event.preventDefault(); ChangePass()">Hello {{UserName}}</a>
        </li>
        <li><a href="#" ng-click="$event.preventDefault(); Logoff()">Log off</a></li>
    </ul>
    <ul ng-show="!IsLoggedIn" class="nav navbar-nav navbar-right">
        <li><a href="/registration">Registration</a></li>
    </ul>
</div>
